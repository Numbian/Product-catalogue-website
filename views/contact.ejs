<% include partials/header%>


<h2 class="text-center mt-4" >Contact us</h2>



<div class="formBg">

<div class="container mt-4 loginform">

<form action="/contact" method="POST">
  
<div class="form-group">
    <label for="Company">Company</label>
    <input name="username" type="text" class="form-control" id="Company">
  </div>

  <div class="form-group">
    <label for="Email">Email</label>
    <input name="email" type="Email" class="form-control" id="Email">
  </div>
  
  <div class="form-group">
    <label for="Area">Message</label>
 <textarea id="Area" class="form-control" name="message"  rows="10" cols="50">
  </textarea>
  </div>

  <button type="submit" class="btn btn-dark my-2">Submit</button>
</form>
    



</div>

<% include partials/footer%>

